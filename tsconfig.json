{
	"compilerOptions": {
		"strict": true,
		"lib": [
			"ES5",
			"ES2015",
			"ES2016",
			"ES2017",
			"DOM"
		],
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
			"@src/*": ["src/*"],
			"@apis/*": ["src/apis/*"],
			"@store": ["src/store"],
			"@store/*": ["src/store/*"],
			"@pages/*": ["src/pages/*"],
			"@hooks/*": ["src/hooks/*"],
			"@constants": ["src/constants"],
			"@constants/*": ["src/constants/*"],
			"@components/*": ["src/components/*"],
			"@public/*": ["public/*"],
			"@images/*": ["public/images/*"],
		}
	}
}